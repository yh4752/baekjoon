#include<stdio.h>

int main() {
	int x, y;

	scanf("%d", &x);
	scanf("%d", &y);

	if (x > 0 && y > 0) { // Quadrant 1
		printf("1");
	}
	else if (x < 0 && y > 0) { // Quadrant 2
		printf("2");
	}
	else if (x < 0 && y < 0) { // Quadrant 3
		printf("3");
	}
	else { // Quadrant 4
		printf("4");
	}

	return 0;
}
